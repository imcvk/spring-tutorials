<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="namelist"
		class="com.springdi.injecting_collection_types.Name_List">
		<constructor-arg>
			<list>
				<value>Sachin</value>
				<value>Suresh</value>
				<value>Vishnu</value>
				<value>Sanjay</value>
			</list>
		</constructor-arg>
	</bean>
	<bean id="nameSet"
		class="com.springdi.injecting_collection_types.Name_Set">
		<constructor-arg>
			<set>
				<value>Sachin</value>
				<value>Suresh</value>
				<value>Vishnu</value>
				<value>Sanjay</value>
			</set>
		</constructor-arg>
	</bean>
	<bean id="nameMap"
		class="com.springdi.injecting_collection_types.Name_Map">
		<constructor-arg>
			<map>
				<entry key="1" value="Suresh"></entry>
				<entry key="2" value="Arjun"></entry>
				<entry key="3" value="Sachin"></entry>
			</map>
		</constructor-arg>
	</bean>
</beans>